{
  "name": "sandrocaputo/novafieldcheckboxes",
  "description": "Adds checkboxes to Laravel Nova fields that works with Relations",
  "keywords": [
    "laravel",
    "nova",
    "input",
    "field",
    "checkbox",
    "checkboxes",
    "multiselect",
    "multiple choice"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "sandrocaputo\\NovaFieldCheckboxes\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "sandrocaputo\\NovaFieldCheckboxes\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}